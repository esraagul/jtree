{
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "destination": "./releases/docsTemp",
    "template": "node_modules/jsdoc-baseline"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
      "name": "jtree",
      "footerText": "owen powell - opowell@gmail.com"
  },
  "source": {
    "include": ["./source"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "tags": {
    "allowUnknownTags": true
  }
}
